{
  "initial_state": "initialize",
  "label": "MainAgent",
  "states": {
    "initialize": {
      "actions": [
        {
          "spawn_agent": {
            "agent_config_file": "agent_config.json",
            "label": "WeatherAgent",
            "is_background": false
          }
        }
      ],
      "next_state": "wait_result"
    },
    "wait_result": {
      "actions": ["wait_for_input"],
      "next_state": "process_result"
    },
    "process_result": {
      "actions": [
        {
          "llm": {
            "user_prompt": "Process the result from the agent. {input}",
            "system_prompt": "You are processing the agent's output."
          }
        }
      ],
      "next_state": null
    }
  }
}
